<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.iwinner.hibenate.model.Employee" table="employee_cri">
		<id name="employeeId" column="empid"/>
		<property name="employeeName" column="empname" type="string"/>
		<property name="employeeSalary" column="empsal" type="int"/>
		<property name="employeedeptNumber" column="deptno" type="int"/>
	</class>
</hibernate-mapping>